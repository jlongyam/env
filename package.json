{
  "name": "@jlongyam/env",
  "version": "4.1.4",
  "description": "cross platform environment detector",
  "keywords": [
    "env",
    "environment"
  ],
  "author": {
    "name": "jlongyam"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jlongyam/env.git"
  },
  "homepage": "https://github.com/jlongyam/env",
  "main": "dist/cjs/env.js",
  "types": "types/env.d.ts",
  "type": "commonjs",
  "engines": {
    "node": ">=5.12.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@jlongyam/repo": "^2.0.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-buble": "^1.0.3",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@web/dev-server": "^0.4.6",
    "bowser": "^2.12.1",
    "rollup": "^4.50.0",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "test:es": "node ./test/test.mjs",
    "build": "rollup -c",
    "test:cjs": "node ./test/test.js",
    "test": "npm run test:es && npm run test:cjs",
    "server": "web-dev-server",
    "types": "tsc",
    "prepare": "repo prepare"
  },
  "exports": {
    ".": {
      "require": "./dist/cjs/env.js",
      "import": "./src/env.mjs",
      "browser": "./dist/iife/env.js",
      "default": "./dist/es/env.js",
      "types": "./types/env.d.ts"
    },
    "./global": {
      "require": "./dist/cjs/envGlobal.js",
      "import": "./src/envGlobal.mjs",
      "browser": "./dist/iife/envGlobal.js",
      "default": "./dist/es/envGlobal.js",
      "types": "./types/envGlobal.d.ts"
    },
    "./browser": {
      "require": "./dist/cjs/envBrowser.js",
      "import": "./src/envBrowser.mjs",
      "browser": "./dist/iife/envBrowser.js",
      "default": "./dist/es/envBrowser.js",
      "types": "./types/envBrowser.d.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "./README.md",
    "./src",
    "./dist",
    "./types",
    "./LICENSE"
  ]
}
