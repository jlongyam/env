{
  "name": "@jlongyam/env",
  "version": "1.1.1",
  "description": "cross platform environment detector",
  "keywords": [
    "env",
    "environment"
  ],
  "author": "jlongyam",
  "license": "MIT",
  "repository": "https://github.com/jlongyam/env",
  "homepage": "https://github.com/jlongyam/env",
  "bugs": "https://github.com/jlongyam/env/issues",
  "main": "./src/envContext.js",
  "type": "module",
  "scripts": {
    "test:source": "node ./test/source.test.js",
    "test:module": "node ./test/module.test.mjs",
    "test:common": "node ./test/common.test.cjs",
    "test": "npm run test:source && npm run test:module && npm run test:common",
    "bundle": "rollup -c",
    "server": "serve ./"
  },
  "devDependencies": {
    "@jlongyam/test": "^0.7.1",
    "@rollup/plugin-buble": "^1.0.3",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "rollup": "^4.48.0",
    "serve": "^14.2.4",
    "systemjs": "^6.15.1"
  },
  "files": [
    "src",
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/mjs/env.mjs",
      "require": "./dist/cjs/env.cjs"
    },
    "./global": {
      "import": "./dist/mjs/envGlobal.mjs",
      "require": "./dist/cjs/envGlobal.cjs"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
