{
  "name": "@jlongyam/env",
  "version": "5.0.2",
  "description": "cross platform environment detector",
  "keywords": [
    "env",
    "environment"
  ],
  "author": {
    "name": "jlongyam"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jlongyam/env.git"
  },
  "homepage": "https://github.com/jlongyam/env",
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@jlongyam/repo": "^2.1.4",
    "@rollup/plugin-babel": "^6.1.0",
    "@rollup/plugin-buble": "^1.0.3",
    "@rollup/plugin-commonjs": "^28.0.9",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@web/dev-server": "^0.4.6",
    "bowser": "^2.12.1",
    "rollup": "^4.57.1",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "typescript": "^5.9.3"
  },
  "main": "dist/env.min.cjs",
  "scripts": {
    "build": "rollup -c --config-minify",
    "types": "tsc",
    "test": "node test/test.cjs && node test/test.mjs",
    "server": "web-dev-server",
    "escheck": "repo escheck ./dist/env.min.cjs ./dist/env.min.js ./dist/envGlobal.min.cjs ./dist/envGlobal.min.js",
    "changelog": "repo changelog"
  },
  "exports": {
    ".": {
      "require": "./dist/env.min.cjs",
      "import": "./dist/env.min.mjs",
      "types": "./dist/env.d.mts"
    },
    "./global": {
      "require": "./dist/envGlobal.min.cjs",
      "import": "./dist/envGlobal.min.mjs",
      "types": "./dist/envGlobal.d.mts"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com/"
  },
  "files": [
    "dist",
    "test",
    "index.html"
  ]
}
